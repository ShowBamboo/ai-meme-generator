### AI Meme Generator Environment Variables

# Default provider (optional): clipdrop, siliconflow, webui, pollinations, mock
IMAGE_GENERATION_PROVIDER=

# Provider order (optional, comma-separated)
IMAGE_GENERATION_PROVIDER_ORDER=clipdrop,siliconflow,webui,pollinations,mock

# Clipdrop (free quota)
CLIPDROP_API_KEY=

# SiliconFlow (OpenAI-compatible image API, usually with free credits)
SILICONFLOW_API_KEY=
SILICONFLOW_API_URL=https://api.siliconflow.cn/v1/images/generations
SILICONFLOW_MODEL=Kwai-Kolors/Kolors
SILICONFLOW_TIMEOUT=180

# Local SD WebUI (AUTOMATIC1111)
SD_WEBUI_URL=http://127.0.0.1:7860
SD_WEBUI_STEPS=20
SD_WEBUI_CFG=7
SD_WEBUI_SAMPLER=Euler a
SD_WEBUI_NEGATIVE=low quality, blurry, watermark

# Pollinations
POLLINATIONS_ENABLED=false

# Caption LLM (optional)
CAPTION_LLM_URL=
CAPTION_LLM_API_KEY=
CAPTION_LLM_MODEL=
CAPTION_LLM_TEMPERATURE=0.9
CAPTION_LLM_TIMEOUT=30
CAPTION_LLM_DEBUG=false
