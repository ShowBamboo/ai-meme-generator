### AI Meme Generator Environment Variables

# Default provider (optional): clipdrop, webui, replicate, huggingface, pollinations, mock
IMAGE_GENERATION_PROVIDER=

# Provider order (optional, comma-separated)
IMAGE_GENERATION_PROVIDER_ORDER=clipdrop,webui,replicate,huggingface,pollinations,mock

# Clipdrop (free quota)
CLIPDROP_API_KEY=

# Local SD WebUI (AUTOMATIC1111)
SD_WEBUI_URL=http://127.0.0.1:7860
SD_WEBUI_STEPS=20
SD_WEBUI_CFG=7
SD_WEBUI_SAMPLER=Euler a
SD_WEBUI_NEGATIVE=low quality, blurry, watermark

# Replicate
REPLICATE_API_TOKEN=

# Hugging Face
HUGGINGFACE_API_TOKEN=

# Pollinations
POLLINATIONS_ENABLED=true

# Caption LLM (optional)
CAPTION_LLM_URL=
CAPTION_LLM_API_KEY=
CAPTION_LLM_MODEL=
CAPTION_LLM_TEMPERATURE=0.9
CAPTION_LLM_TIMEOUT=30
CAPTION_LLM_DEBUG=false
